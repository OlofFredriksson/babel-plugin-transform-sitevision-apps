{
  "name": "babel-plugin-transform-modules-sitevision",
  "version": "0.0.0",
  "description": "This plugins transforms ES2015 modules to Sitevision modules\"",
  "main": "dist/transform.js",
  "scripts": {
    "build": "mkdir -p dist && babel src/transform.js -o dist/transform.js",
    "clean": "rm -rf dist",
    "eslint": "eslint src/**/*.js tests/**/*.js",
    "postversion": "git push --follow-tags",
    "prettier:check": "prettier '**/*.{js,json,md}' --list-different",
    "prettier:write": "prettier '**/*.{js,json,md}' --write",
    "version": "sv-update-changelog -i && git add CHANGELOG.md",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.sidvind.com:422/ext/babel-plugin-transform-modules-sitevision.git"
  },
  "keywords": [
    "babel-plugin"
  ],
  "author": "David Sveningsson <ext@sidvind.com>",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@babel/helper-module-transforms": "^7.4.4",
    "@babel/helper-plugin-utils": "^7.0.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@sidvind/build-scripts": "1.0.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-sidvind": "1.3.2",
    "eslint-plugin-jest": "22.7.1",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "^2.7.0",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "lint-staged": "^8.2.1",
    "prettier": "1.18.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "output": "jest.junit.xml"
        }
      ]
    ]
  }
}
